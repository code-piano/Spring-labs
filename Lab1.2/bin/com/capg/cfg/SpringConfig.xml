<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean class="com.capg.bean.Employee" id="emp">
	<property name="empId" value="12345"/>
	<property name="empName" value="Naman"/>
	<property name="salary" value="40000"/>
	<property name="businUnit" value="PES-BU"/>
	<property name="age" value="30"/>
	</bean>
	
	<bean class="com.capg.bean.SBU" id="sbu">
	<property name="sbuId" value="007"/>
	<property name="sbuName" value="Product Engineering Services"/>
	<property name="sbuHead" value="Mr. Raj Arora"/>
	</bean>
	
	
	
	
	
	
	
</beans>
